# Basic process stuff
alias tu='top -o cpu' # cpu
alias tm='top -o vsize' # memory

# Git
alias gst="git status"
alias ga="git add"
alias gb="git branch"
alias gc="git commit -v"
alias gac="git commit -v -a"
alias gd="git diff"
alias go="git checkout"
alias gk="gitk --all&"

# Misc
alias lf="ls -F"
alias ll="ls -al"

# Slicehost
alias sftp-feynman='sftp -oPort=33333 kevin@174.143.235.201'
alias ssh-feynman='ssh -p 33333 kevin@174.143.235.201'
